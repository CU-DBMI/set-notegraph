{
    "$id": "https://cuhealth.ai/business/2022-06-28/effort_hours-client.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "effort hours Client schema",
    "additionalProperties": false,
    "type": "object",
    "title": "A Client",
    "required": [
        "MDC Client ID",
        "Client Name",
        "No Bill",
        "Projects"
    ],
    "properties": {
        "MDC Client ID": {
            "type": "string",
            "title": "The MDC Client ID"
        },
        "Client Name": {
            "type": "string",
            "title": "The MDC Client/Client name"
        },
        "No Bill": {
            "type": "boolean",
            "title": "A flag that indicates if the Client should be billed"
        },
        "Notes": {
            "type": [
                "string",
                "null"
            ],
            "title": "Optional Client Notes"
        },
        "Projects": {
            "type": "array",
            "title": "A list of projects",
            "items": {
                "$ref": "schemas/effort_hours-project.json"
            }
        }
    }
}