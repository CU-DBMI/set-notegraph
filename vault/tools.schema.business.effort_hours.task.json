{
    "$id": "https://cuhealth.ai/business/2022-06-28/effort_hours-task.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "A task",
    "additionalProperties": false,
    "type": "object",
    "title": "Task",
    "required": [
        "MDC Task ID",
        "Task Name",
        "Completed Hours",
        "Task Complete Date",
        "Session Complete Date",
        "Integration Message"
    ],
    "properties": {
        "MDC Task ID": {
            "type": "string",
            "title": "The MDC Task ID"
        },
        "Task Name": {
            "type": "string",
            "title": "The name of the task"
        },
        "Completed Hours": {
            "type": "number",
            "title": "Completed hours for this task for the report"
        },
        "Issue URL": {
            "type": [
                "string",
                "null"
            ],
            "title": "Git issue URL"
        },
        "Pull Request URL": {
            "type": [
                "string",
                "null"
            ],
            "title": "Git PR URL"
        },
        "Notes": {
            "type": [
                "string",
                "null"
            ],
            "title": "Optional Project Notes"
        },
        "Task Complete Date": {
            "type": "string",
            "format": "date",
            "title": "The date the entire task is completed"
        },
        "Session Complete Date": {
            "type": "string",
            "format": "date",
            "title": "The date a single session in a task is completed"
        },
        "Integration Message": {
            "type": [
                "string",
                "null"
            ],
            "title": "Optional integration message"
        }
    }
}